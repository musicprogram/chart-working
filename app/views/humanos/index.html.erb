<p id="notice"><%= notice %></p>

<h1>Listing Humanos</h1>

<table>
  <thead>
    <tr>
      <th>Mujer</th>
      <th>Hombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @humanos.each do |humano| %>
      <tr>
        <td><%= humano.mujer %></td>
        <td><%= humano.hombre %></td>
        <td><%= link_to 'Show', humano %></td>
        <td><%= link_to 'Edit', edit_humano_path(humano) %></td>
        <td><%= link_to 'Destroy', humano, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Humano', new_humano_path %>



<style>
		
		.container{
			width:100%; 
			height:400px;
		}

	</style>
	<script src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js"></script>
   
	
	<script>
	  	$(function () {
	    $('#container').highcharts({
	        chart: {
	            type: 'line'
	        },
	        title: {
	            text: 'Fruit Consumption'
	        },
	        xAxis: {
	          
	            categories: ['Apples', 'Bananas', 'Oranges']
	        },
	        yAxis: {
	            title: {
	                text: 'Fruit eaten'
	            }
	        },
	        series: [{
	            name: 'Mujer',
	            <% data = Humano.all.map(&:mujer) %> 
	            data: <%= data %>
	        }, {
	            name: 'Hombre',
	            <% data1 = Humano.all.map(&:hombre)  %>
	            data: <%= data1 %>
	        }]
	    });
	});

	</script>

</head>
<body>
	<h1>Charts</h1>

	<div id="container"></div>